<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="600.0" maxWidth="800.0" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" stylesheets="@css/main.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="asu.onlinebankinggui.MainMenuPageController">
   <children>
      <ImageView fitHeight="200.0" fitWidth="200.0" layoutX="300.0" layoutY="150.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/BankIcon.png" />
         </image>
      </ImageView>
      <Button layoutX="250.0" layoutY="350.0" mnemonicParsing="false" onAction="#onMyAccountsButtonClick" prefHeight="40.0" prefWidth="125.0" text="My Accounts">
         <font>
            <Font name="Dubai Regular" size="14.0" />
         </font>
      </Button>
      <Button layoutX="450.0" layoutY="350.0" mnemonicParsing="false" onAction="#onTransactionsButtonClick" prefHeight="40.0" prefWidth="125.0" text="Transactions" textAlignment="CENTER">
         <font>
            <Font name="PT Bold Heading" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
