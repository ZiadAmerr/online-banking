<AnchorPane maxHeight="600.0" maxWidth="800.0" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" stylesheets="@css/main.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="asu.onlinebankinggui.SignupPageController">
   <children>
      <ImageView fitHeight="150.0" fitWidth="150.0" layoutX="325.0" layoutY="58.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/BankIcon.png" />
         </image>
      </ImageView>
      <Button layoutX="275.0" layoutY="450.0" mnemonicParsing="false" onAction="#onSignupButtonClick" prefHeight="41.0" prefWidth="94.0" text="Signup">
         <font>
            <Font name="Dubai Regular" size="14.0" />
         </font>
      </Button>
      <Button fx:id="cancel" layoutX="425.0" layoutY="450.0" mnemonicParsing="false" onAction="#onCancelButtonClick" prefHeight="41.0" prefWidth="94.0" styleClass="back_button" text="Cancel" textAlignment="CENTER">
         <font>
            <Font name="PT Bold Heading" size="14.0" />
         </font>
      </Button>
      <Label layoutX="225.0" layoutY="350.0" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Email" />
      <Label layoutX="225.0" layoutY="400.0" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Password" />
      <TextField fx:id="email" layoutX="326.0" layoutY="350.0" maxHeight="30.0" maxWidth="225.0" minHeight="30.0" minWidth="225.0" prefHeight="30.0" prefWidth="225.0" />
      <PasswordField fx:id="password" layoutX="326.0" layoutY="400.0" maxHeight="30.0" maxWidth="225.0" minHeight="30.0" minWidth="225.0" prefHeight="30.0" prefWidth="225.0" />
      <Label layoutX="225.0" layoutY="250.0" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="First Name" />
      <Label layoutX="225.0" layoutY="300.0" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Last Name" />
      <TextField fx:id="lastName" layoutX="325.0" layoutY="300.0" maxHeight="30.0" maxWidth="225.0" minHeight="30.0" minWidth="225.0" prefHeight="30.0" prefWidth="225.0" />
      <TextField fx:id="firstName" layoutX="325.0" layoutY="250.0" maxHeight="30.0" maxWidth="225.0" minHeight="30.0" minWidth="225.0" prefHeight="30.0" prefWidth="225.0" />
